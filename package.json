{
  "name": "reactjs-exercise-api",
  "version": "1.0.0",
  "description": "Basic NodeJS REST API that writes objects to a mongo db and connect with a third-party service",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
    "serve": "node dist/server.js",
    "start_babel": "babel-node ./server.js --preset=babel-preset-es2015",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askochypets/ReactJS-Developers-Exercise-API.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/askochypets/ReactJS-Developers-Exercise-API/issues"
  },
  "homepage": "https://github.com/askochypets/ReactJS-Developers-Exercise-API#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "4.14.0",
    "mongoose": "^4.8.6"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-inline-import": "^2.0.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
